{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "has": [
        { "type": "header", "key": "accept", "value": ".*text/html.*" }
      ],
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/panel",
      "destination": "/",
      "permanent": true
    }
  ]
}
